{
  "name": "@qiwi/nestjs-enterprise-metric",
  "version": "1.6.1",
  "description": "Metric module for nestjs enterprise",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ]
  },
  "main": "./target/es6/index.js",
  "exports": "./target/es6/index.js",
  "types": "target/es6/index.d.ts",
  "typescript": {
    "definition": "target/es6/index.d.ts"
  },
  "scripts": {
    "clean": "npx packasso clean",
    "build": "npx packasso build",
    "test": "npx packasso test",
    "lint": "npx packasso lint",
    "lint:fix": "npx packasso lint --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/decorator-utils": "^4.5.4",
    "graphite": "^0.1.5",
    "measured-reporting": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "@types/graphite": "^0.1.0",
    "@types/lodash": "^4.14.198",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "metric"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT"
}
