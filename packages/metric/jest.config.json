{
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest",
      {
        "useESM": true,
        "tsconfig": "<rootDir>/tsconfig.test.json"
      }
    ]
  },
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/main/**/*.(j|t)s"
  ],
  "testMatch": [
    "<rootDir>/src/test/**/!(*.node).(j|t)s"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "preset": "ts-jest",
  "extensionsToTreatAsEsm": [
    ".ts",
    ".d.ts"
  ]
}
