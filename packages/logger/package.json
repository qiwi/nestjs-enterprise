{
  "name": "@qiwi/nestjs-enterprise-logger",
  "version": "1.6.1",
  "description": "Logger module for nestjs enterprise",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ]
  },
  "exports": "./src/main/ts/index.ts",
  "scripts": {
    "clean": "npx packasso clean",
    "build": "npx packasso build",
    "test": "PACKASSO=node_test npx packasso test",
    "lint": "npx packasso lint",
    "lint:fix": "npx packasso lint --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/logwrap": "^1.5.1",
    "@qiwi/mware-context": "^1.14.0",
    "@qiwi/substrate": "^2.0.4",
    "@types/winston": "^2.4.4",
    "fast-luhn": "^2.0.1",
    "fast-safe-stringify": "^2.1.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "@types/lodash": "^4.14.198",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "tempy": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "logger"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT"
}
