{
  "name": "@qiwi/nestjs-enterprise-logger",
  "version": "1.6.1",
  "description": "Logger module for nestjs enterprise",
  "publishConfig": {
    "access": "public"
  },
  "main": "target/es5/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es5/index.d.ts",
  "typescript": {
    "definition": "target/es5/index.d.ts"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target",
    "typings",
    "flow-typed",
    "buildstamp.json"
  ],
  "scripts": {
    "test": "yarn lint && yarn test:spec",
    "test:gen": "yarn jest --updateSnapshot",
    "test:spec": "yarn jest",
    "jest": "yarn infra:jest -w=1 --config=jest.config.json",
    "format": "yarn infra:prettier --write \"src/**/*.ts\"",
    "lint": "yarn infra:eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn infra:rimraf target typings buildcache flow-typed docs buildstamp.json",
    "prebuild": "yarn infra:buildstamp --git --out.path buildstamp.json",
    "build": "yarn prebuild && yarn build:es5 && yarn build:es6 && yarn build:ts && yarn build:libdef && yarn docs && yarn uglify",
    "build:es5": "mkdir -p target/es5 && yarn infra:tsc -p tsconfig.es5.json",
    "build:es6": "mkdir -p target/es6 && yarn infra:tsc -p tsconfig.es6.json",
    "build:ts": "cp -r src/main/ts/ target/ts/",
    "build:bundle": "yarn infra:microbundle build src/main/ts/index.ts -o target/bundle",
    "build:libdef": "yarn infra:libdefkit --tsconfig=tsconfig.es5.json --tsconfig=tsconfig.es6.json",
    "docs": "yarn infra:typedoc",
    "uglify": "yarn infra:uglify",
    "postupdate": "yarn && yarn build && yarn test"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.5",
    "@nestjs/core": "^9.0.5",
    "@qiwi/logwrap": "^1.5.1",
    "@qiwi/mware-context": "^1.13.0",
    "@qiwi/substrate": "^1.20.14",
    "@types/winston": "^2.4.4",
    "fast-luhn": "^2.0.0",
    "fast-safe-stringify": "^2.1.1",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@qiwi/nestjs-enterprise-infra": "1.3.1",
    "@types/bluebird": "^3.5.36",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "esm": "^3.2.25",
    "reflect-metadata": "^0.1.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "logger"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme"
}
