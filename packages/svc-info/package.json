    "lint:fix": "npx packasso lint --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "resolve-cwd": "^3.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/testing": "^10.2.6",
    "@types/bluebird": "^3.5.38",
    "@types/rimraf": "^4.0.5",
    "@types/supertest": "^2.0.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
