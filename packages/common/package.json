{
  "name": "@qiwi/nestjs-enterprise-common",
  "version": "1.4.1",
  "description": "QIWI Nestjs enterprise common assets",
  "publishConfig": {
    "access": "public"
  },
  "main": "./target/es6/index.js",
  "exports": "./target/es6/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es6/index.d.ts",
  "typescript": {
    "definition": "target/es6/index.d.ts"
  },
  "type": "module",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target",
    "typings",
    "flow-typed",
    "buildstamp.json"
  ],
  "scripts": {
    "test": "yarn lint && yarn test:node",
    "test:node": "node --loader tsx --test **/*.spec.ts",
    "format": "yarn infra:prettier --write \"src/**/*.ts\"",
    "lint": "yarn infra:eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn infra:rimraf target typings buildcache flow-typed docs buildstamp.json",
    "prebuild": "cp ../../buildstamp.json ./",
    "build": "yarn prebuild && yarn build:es6 && yarn build:ts && yarn docs && yarn uglify",
    "build:es6": "mkdir -p target/es6 && yarn infra:tsc -p tsconfig.es6.json && yarn infra:tsc-esm-fix --target='target/es6'",
    "build:ts": "cp -r src/main/ts/ target/ts/",
    "docs": "yarn infra:typedoc",
    "uglify": "yarn infra:uglify",
    "postupdate": "yarn && yarn build && yarn test"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/decorator-utils": "^4.5.4",
    "graphite": "^0.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.1.3",
    "@qiwi/nestjs-enterprise-infra": "workspace:*",
    "@types/express": "^4.17.17",
    "@types/graphite": "^0.1.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "inside-out-promise": "^2.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "decorator",
    "common"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme"
}
