    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/testing": "^10.2.6",
    "@types/express": "^4.17.17",
    "@types/graphite": "^0.1.0",
    "@types/lodash": "^4.14.198",
    "@types/supertest": "^2.0.12",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
