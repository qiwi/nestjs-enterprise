{
  "name": "@qiwi/nestjs-enterprise-common",
  "version": "1.4.1",
  "description": "QIWI Nestjs enterprise common assets",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
