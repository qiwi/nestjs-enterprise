{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src/main/ts/",
    "baseUrl": "./src/main/ts/",
    "target": "es5",
    "outDir": "target/es5",
    "tsBuildInfoFile": "./buildcache/.tsbuildinfo",
    "downlevelIteration": true
  },
  "include": [
    "src/main/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
