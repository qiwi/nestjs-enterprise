{
  "name": "@qiwi/nestjs-enterprise-consul",
  "version": "1.3.1",
  "description": "Nestjs module for working with Consul",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ]
  },
  "exports": "./src/main/ts/index.ts",
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/consul-service-discovery": "^1.10.6",
    "@qiwi/substrate": "^2.0.4",
    "@types/lodash": "^4.14.198",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "@qiwi/nestjs-enterprise-connection-provider": "workspace:*",
    "@types/bluebird": "^3.5.38",
    "@types/consul": "^0.40.0",
    "@types/lodash": "^4.14.198",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "consul"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT"
}
