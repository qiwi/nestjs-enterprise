{
  "name": "@qiwi/nestjs-enterprise-thrift",
  "version": "1.3.1",
  "description": "thrift module for nestjs enterprise",
  "publishConfig": {
    "access": "public"
  },
  "main": "./target/es6/index.js",
  "exports": "./target/es6/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es6/index.d.ts",
  "typescript": {
    "definition": "target/es6/index.d.ts"
  },
  "type": "module",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target",
    "typings",
    "flow-typed",
    "buildstamp.json"
  ],
  "scripts": {
    "test": "yarn lint && yarn test:node",
    "test:gen": "yarn jest --updateSnapshot",
    "test:spec": "yarn jest",
    "test:node": "node --loader tsx --test **/*.node.ts",
    "jest": "NODE_OPTIONS=--experimental-vm-modules yarn infra:jest -w=1 --config=jest.config.json",
    "format": "yarn infra:prettier --write \"src/**/*.ts\"",
    "lint": "yarn infra:eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn infra:rimraf target typings buildcache flow-typed docs buildstamp.json",
    "build": "yarn buildstamp && yarn build:es6 && yarn build:ts && yarn docs && yarn uglify",
    "build:es6": "mkdir -p target/es6 && yarn infra:tsc -p tsconfig.es6.json && yarn infra:tsc-esm-fix --target='target/es6'",
    "build:ts": "cp -r src/main/ts/ target/ts/",
    "buildstamp": "cp ../../buildstamp.json ./",
    "docs": "yarn infra:typedoc",
    "uglify": "yarn infra:uglify",
    "postupdate": "yarn && yarn build && yarn test"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@qiwi/substrate": "^2.0.4",
    "@types/thrift": "^0.10.12",
    "generic-pool": "^3.9.0",
    "inside-out-promise": "^2.1.5",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "thrift": "0.18.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@qiwi/nestjs-enterprise-config": "workspace:*",
    "@qiwi/nestjs-enterprise-connection-provider": "workspace:*",
    "@qiwi/nestjs-enterprise-consul": "workspace:*",
    "@qiwi/nestjs-enterprise-infra": "workspace:*",
    "@qiwi/nestjs-enterprise-logger": "workspace:*",
    "@types/generic-pool": "^3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "thrift"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme"
}
