{
  "name": "@qiwi/nestjs-enterprise-thrift",
  "version": "1.3.1",
  "description": "thrift module for nestjs enterprise",
  "type": "module",
  "exports": "./src/main/ts/index.ts",
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/substrate": "^2.0.4",
    "@types/generic-pool": "^3.8.1",
    "@types/thrift": "^0.10.12",
    "generic-pool": "^3.9.0",
    "inside-out-promise": "^2.1.5",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "thrift": "0.18.1"
  },
  "devDependencies": {
    "@qiwi/nestjs-enterprise-config": "workspace:*",
    "@qiwi/nestjs-enterprise-connection-provider": "workspace:*",
    "@qiwi/nestjs-enterprise-consul": "workspace:*",
    "@qiwi/nestjs-enterprise-logger": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "thrift"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT",
  "publishConfig": {
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ],
    "access": "public"
  }
}
