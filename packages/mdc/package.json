{
  "name": "@qiwi/nestjs-enterprise-mdc",
  "version": "1.4.1",
  "description": "MDC module for nestjs enterprise",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ]
  },
  "exports": "./src/main/ts/index.ts",
  "scripts": {
    "clean": "npx packasso clean",
    "build": "npx packasso build",
    "test": "npx packasso test",
    "lint": "npx packasso lint",
    "lint:fix": "npx packasso lint --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/mware": "^1.14.2",
    "@qiwi/substrate": "^2.0.4",
    "@types/lodash": "^4.14.198",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT"
}
