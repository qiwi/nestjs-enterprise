{
  "name": "@qiwi/nestjs-enterprise-config",
  "version": "1.4.1",
  "description": "Config module for nestjs enterprise",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ]
  },
  "exports": "./src/main/ts/index.ts",
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@qiwi/uniconfig": "^3.5.14",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme",
  "license": "MIT"
}
