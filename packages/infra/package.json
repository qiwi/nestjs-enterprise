{
  "name": "@qiwi/nestjs-enterprise-infra",
  "version": "1.3.1",
  "description": "QIWI Nestjs enterprise infra toolkit",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md"
  ],
  "scripts": {
    "postupdate": "yarn",
    "infra:tsc": "cd $INIT_CWD && tsc",
    "infra:typedoc": "cd $INIT_CWD && typedoc --options typedoc.cjs",
    "infra:uglify": "./scripts/sh/uglify.sh",
    "infra:eslint": "cd $INIT_CWD && eslint",
    "infra:prettier": "cd $INIT_CWD && prettier",
    "infra:jest": "cd $INIT_CWD && NODE_OPTIONS=--experimental-vm-modules jest",
    "infra:rimraf": "cd $INIT_CWD && rimraf",
    "infra:buildstamp": "cd $INIT_CWD && buildstamp",
    "infra:tsc-esm-fix": "cd $INIT_CWD && tsc-esm-fix"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.5",
    "@swissquote/crafty-preset-jest": "^1.19.1",
    "@types/jest": "^28.1.6",
    "@types/jest-json-schema": "^6.1.1",
    "@types/supertest": "^2.0.12",
    "buildstamp": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-qiwi": "^1.17.4",
    "esm": "^3.2.25",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "terser": "^5.14.2",
    "ts-jest": "^28.0.7",
    "tsc-esm-fix": "^2.20.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme"
}
