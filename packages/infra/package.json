{
  "name": "@qiwi/nestjs-enterprise-infra",
  "version": "1.3.1",
  "description": "QIWI Nestjs enterprise infra toolkit",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md"
  ],
  "scripts": {
    "postupdate": "yarn",
    "infra:tsc": "cd $INIT_CWD && tsc",
    "infra:typedoc": "cd $INIT_CWD && typedoc --options typedoc.cjs",
    "infra:uglify": "./scripts/sh/uglify.sh",
    "infra:eslint": "cd $INIT_CWD && eslint",
    "infra:prettier": "cd $INIT_CWD && prettier",
    "infra:rimraf": "cd $INIT_CWD && rimraf",
    "infra:buildstamp": "cd $INIT_CWD && buildstamp",
    "infra:tsc-esm-fix": "cd $INIT_CWD && tsc-esm-fix"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.1.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.4.9",
    "@types/supertest": "^2.0.12",
    "buildstamp": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-qiwi": "^2.1.3",
    "esm": "^3.2.25",
    "prettier": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "terser": "^5.19.2",
    "tsc-esm-fix": "^2.20.14",
    "typedoc": "^0.24.8",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "keywords": [
    "js platform",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/qiwi/nestjs-enterprise/issues"
  },
  "homepage": "https://github.com/qiwi/nestjs-enterprise#readme"
}
