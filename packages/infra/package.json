{
  "name": "@qiwi/nestjs-enterprise-infra",
  "version": "1.3.1",
  "description": "QIWI Nestjs enterprise infra toolkit",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
