{
  "name": "@qiwi/nestjs-enterprise-monorepo",
  "version": "0.0.0",
  "description": "QIWI Nestjs Enterprise",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "prepare": "yarn install && npx packasso install",
    "clean": "npx packasso clean",
    "build": "npx packasso build",
    "build-ci": "npx packasso build",
    "test": "PACKASSO=node_test npx packasso test",
    "test:report": "PACKASSO=node_test npx packasso test",
    "lint": "npx packasso lint",
    "lint:fix": "npx packasso lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/nestjs-enterprise.git"
  },
  "packageManager": "yarn@4.0.0-rc.13",
  "packasso": "@packasso/lib",
  "license": "MIT"
}
